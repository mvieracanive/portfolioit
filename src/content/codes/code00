const funToWaitFor = async (arg1) => {
    return new Promise((resolve, reject) => {
      setTimeout(() => {
        resolve(arg1);
      }, 2000);
    });
  };

return (
    <div className="App">
        <ModalWait
            onExeFun={funToWaitFor}
            args={["Arg1: Success"]}
            handleResponse={handleResponse}
        />
    </div>);